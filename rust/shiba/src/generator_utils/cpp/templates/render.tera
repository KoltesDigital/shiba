{% if project_codes.rendering %}
	{{ project_codes.rendering }}
{% else %}
	#ifdef shibaUniformTime
		shibaUniformTime = shibaTime;
	#endif

	#ifdef shibaUniformResolutionWidth
		shibaUniformResolutionWidth = shibaResolutionWidth;
	#endif

	#ifdef shibaUniformResolutionHeight
		shibaUniformResolutionHeight = shibaResolutionHeight;
	#endif

	{% if target == "blender_api" %}
		glUseProgram(shibaProgram);
		shibaCheckGlError();
	{% endif %}

	glUniform1fv(0, shibaFloatUniformCount, shibaFloatUniforms);
	shibaCheckGlError();

	shibaDrawScreenRect();
	shibaCheckGlError();
{% endif %}
